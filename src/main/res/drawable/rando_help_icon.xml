<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/rando_red" />
            <size
                android:width="30dp"
                android:height="30dp" />
        </shape>
    </item>
   <item
       android:top="10dp"
       android:left="10dp"
       android:right="10dp"
       android:bottom="10dp"
       android:gravity="center">
        <shape android:shape="oval">
            <stroke
                android:width="@dimen/rando_help_icon_thickness"
                android:color="#ffffffff" />
            <solid android:color="@color/rando_red" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
</layer-list>